{
  "name": "qualitizer",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "standalone": "cross-env VITE_STANDALONE=true vite",
    "standalone-internal": "cross-env VITE_STANDALONE=true VITE_SHOW_INTERNAL=true vite",
    "proxy": "node server/proxy.mjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "deploy": "pnpm run build && echo 'Deploying...' && echo 'Deployed!'",
    "deploy-preview": "pnpm run build && echo 'Deploying preview...' && echo 'Deployed preview!'"
  },
  "dependencies": {
    "@cognite/dune": "^0.2.2",
    "@cognite/sdk": "^10.5.0",
    "@fastify/cors": "^11.2.0",
    "@tanstack/react-query": "^5.90.20",
    "@types/d3-chord": "^3.0.6",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-shape": "^3.1.8",
    "clsx": "^2.1.1",
    "d3-chord": "^3.0.1",
    "d3-hierarchy": "^3.1.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "dotenv": "^17.2.4",
    "fastify": "^5.7.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "undici": "^7.21.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "cross-env": "^10.1.0",
    "happy-dom": "^20.5.3",
    "postcss": "^8.5.6",
    "shadcn": "^3.8.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
